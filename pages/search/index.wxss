page{
  background-color: #ddd;
}
.top{
  width: 680rpx;
  
  height: 60rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.cancel{
  background-color: white;
  display: inline-block;
  height: 60rpx;
  width: 110rpx;
  vertical-align: top;
  margin-left: 20rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 60rpx;
  border-radius: 5rpx;

}
.inputsearch{
  background-color: white;
  display: inline-block;
  width: 550rpx;
  height: 60rpx;
  vertical-align: top;
  box-sizing: border-box;
  line-height: 60rpx;
  border-radius: 5rpx;
  padding: 0 20rpx;
}
.item{
  width: 680rpx;
  height: 60rpx;
  margin: 0 auto;
  border-bottom: 1px solid #ddd;
  line-height: 60rpx;
  word-break:break-all;
	display:-webkit-box;
	-webkit-line-clamp:1;
	-webkit-box-orient:vertical;
  overflow:hidden;
  background-color: white;
  padding: 0 20rpx;
}
.top .longWidth{
  width: 100%;
}